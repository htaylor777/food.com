    text-decoration: none;
    background-color: rgb(11, 47, 81);
  }

.post-item {
    border-bottom: 1px dotted #dedede;
    padding-bottom: 1.7rem;
    margin-bottom: 1.7rem;
  }

.post-item:last-of-type {
    border-bottom: none;
    margin-bottom: 0;
